{
    "name": "cli-adventure-games",
    "description": "Play Text based Adventures in your terminal",
    "version": "1.0.0",
    "author": "Arvind Ravulavaru @arvindr21",
    "bin": {
        "cli-adventure-games": "./bin/run"
    },
    "bugs": "https://github.com/arvindr21/cli-adventure-games/issues",
    "dependencies": {
        "@oclif/command": "^1.8.0",
        "@oclif/config": "^1.17.0",
        "@oclif/plugin-help": "^3.2.2",
        "JSONStream": "^1.3.5",
        "inquirer": "^8.0.0",
        "tslib": "^1.14.1"
    },
    "devDependencies": {
        "@oclif/dev-cli": "^1.26.0",
        "@oclif/test": "^1.2.8",
        "@types/chai": "^4.2.18",
        "@types/inquirer": "^7.3.1",
        "@types/jsonstream": "^0.8.30",
        "@types/mocha": "^5.2.7",
        "@types/node": "^10.17.60",
        "chai": "^4.3.4",
        "eslint": "^7.26.0",
        "eslint-config-oclif": "^3.1.0",
        "eslint-config-oclif-typescript": "^0.1.0",
        "eslint-config-standard": "^16.0.2",
        "eslint-plugin-import": "^2.23.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^4.3.1",
        "globby": "^10.0.2",
        "mocha": "^5.2.0",
        "nyc": "^14.1.1",
        "ts-node": "^8.10.2",
        "typescript": "^3.9.9"
    },
    "engines": {
        "node": ">=8.0.0"
    },
    "files": [
        "/bin",
        "/lib",
        "/npm-shrinkwrap.json",
        "/oclif.manifest.json"
    ],
    "homepage": "https://github.com/arvindr21/cli-adventure-games",
    "keywords": [
        "oclif"
    ],
    "license": "MIT",
    "main": "lib/index.js",
    "oclif": {
        "commands": "./lib/commands",
        "bin": "cli-adventure-games",
        "plugins": [
            "@oclif/plugin-help"
        ]
    },
    "repository": "arvindr21/cli-adventure-games",
    "scripts": {
        "postpack": "rm -f oclif.manifest.json",
        "posttest": "eslint . --ext .ts --config .eslintrc",
        "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
        "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
        "version": "oclif-dev readme && git add README.md"
    },
    "types": "lib/index.d.ts"
}
